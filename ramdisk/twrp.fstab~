# mount point  	fstype    	device				    device2	
/efs		ext4	/dev/block/mmcblk0p15 flags=display="EFS";backup=1	     
/system		ext4	/dev/block/mmcblk0p7  							
/cache		ext4	/dev/block/mmcblk0p8							
/data		ext4	/dev/block/mmcblk0p9			  length=-16384
/sdcard 	vfat 	/dev/block/mmcblk0p11 /dev/block/mmcblk0  flags=display="Internal Storage";wipeingui 
/sdcard1   	vfat   /dev/block/mmcblk1p1   /dev/block/mmcblk1  flags=display="Micro SDcard";storage;wipeingui;removable

